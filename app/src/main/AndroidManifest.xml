<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StreamSharing"
        tools:targetApi="31">
        <activity
            android:name="com.example.streamsharing.ui.theme.PruebaComposeTheme"
            android:exported="false"
            android:label="@string/title_activity_prueba_compose_theme"
            android:theme="@style/Theme.StreamSharing" />
        <activity
            android:name="com.example.streamsharing.RegistroScreen"
            android:exported="false"
            android:label="@string/title_activity_registro_screen"
            android:theme="@style/Theme.StreamSharing" />
        <activity
            android:name="com.example.streamsharing.AgregarAmigosScreen"
            android:exported="false"
            android:label="@string/title_activity_agregar_amigos_screen"
            android:theme="@style/Theme.StreamSharing" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>